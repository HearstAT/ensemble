<p id="notice"><%= notice %></p>

<h1>New Relic Configs</h1>

<table>
  <thead>
    <tr>
      <th>Business unit</th>
      <th>Account number</th>
      <th>Api key</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @new_relic_configs.each do |new_relic_config| %>
      <tr>
        <td><%= new_relic_config.business_unit.name %></td>
        <td><%= new_relic_config.account_number %></td>
        <td><%= new_relic_config.api_key %></td>
        <td><%= link_to 'Show', new_relic_config %></td>
        <td><%= link_to 'Edit', edit_new_relic_config_path(new_relic_config) %></td>
        <td><%= link_to 'Destroy', new_relic_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New New Relic Config', new_new_relic_config_path %>
